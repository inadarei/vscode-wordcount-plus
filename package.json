{
	"name": "wordcount-plus",
	"displayName": "Word Count Plus",
	"version": "0.1.0",
	"publisher": "inadarei",
	"description": "Word Count Fork With More File Formats Supported",
	"categories": [
		"Other"
	],
	"license": "SEE LICENSE IN LICENSE.txt",
	"icon": "images/icon.png",
	"galleryBanner": {
		"color": "#C80000",
		"theme": "dark"
	},
	"activationEvents": [
		"onLanguage:markdown",
		"onLanguage:asciidoc",
		"onLanguage:plaintext"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/inadarei/vscode-wordcount-plus"
	},
	"engines": {
		"vscode": "0.10.x"
	},
	"main": "./out/extension",
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"vscode": "0.10.x",
		"typescript": "^1.6.2"
	}
}
